{
  "name": "prettier-plugin-brainfuck",
  "version": "0.2.0",
  "description": "prettier plugin for brainfuck",
  "main": "dist/plugin.js",
  "scripts": {
    "compile": "tsc --noEmit",
    "evaluate": "ts-node bin/evaluate.ts",
    "lint": "eslint --cache --ext .ts .",
    "parse": "ts-node bin/parse.ts",
    "prepublishOnly": "rm -f dist/* && yarn tsc --declaration",
    "print": "ts-node bin/print.ts",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kddnewton/prettier-plugin-brainfuck.git"
  },
  "author": "Kevin Newton",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kddnewton/prettier-plugin-brainfuck/issues"
  },
  "homepage": "https://github.com/kddnewton/prettier-plugin-brainfuck#readme",
  "dependencies": {
    "prettier": "^2.5.1"
  },
  "devDependencies": {
    "@types/jest": "^27.0.0",
    "@types/node": "^16.11.12",
    "@types/prettier": "^2.3.1",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.0.0",
    "jest": "^27.4.3",
    "ts-jest": "^27.1.1",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5"
  },
  "eslintConfig": {
    "extends": [
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_",
          "varsIgnorePattern": "^_"
        }
      ]
    }
  },
  "eslintIgnore": [
    "/dist/"
  ],
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "prettier": {
    "trailingComma": "none"
  }
}
