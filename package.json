{
  "name": "prettier-plugin-brainfuck",
  "version": "1.0.0",
  "description": "prettier plugin for brainfuck",
  "type": "module",
  "main": "src/plugin.js",
  "scripts": {
    "evaluate": "node bin/evaluate.js",
    "lint": "eslint --cache .",
    "parse": "node bin/parse.js",
    "print": "node bin/print.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kddnewton/prettier-plugin-brainfuck.git"
  },
  "author": "Kevin Newton",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kddnewton/prettier-plugin-brainfuck/issues"
  },
  "homepage": "https://github.com/kddnewton/prettier-plugin-brainfuck#readme",
  "dependencies": {
    "prettier": ">=3.0.0-alpha.3"
  },
  "devDependencies": {
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.0.1",
    "jest": "^30.0.4"
  },
  "jest": {
    "transform": {}
  },
  "prettier": {
    "plugins": [
      "./src/plugin.js"
    ],
    "trailingComma": "none"
  }
}
