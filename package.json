{
  "name": "prettier-plugin-brainfuck",
  "version": "1.0.0",
  "description": "prettier plugin for brainfuck",
  "type": "module",
  "main": "src/plugin.js",
  "scripts": {
    "evaluate": "node bin/evaluate.js",
    "lint": "eslint --cache .",
    "parse": "node bin/parse.js",
    "print": "node bin/print.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kddnewton/prettier-plugin-brainfuck.git"
  },
  "author": "Kevin Newton",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kddnewton/prettier-plugin-brainfuck/issues"
  },
  "homepage": "https://github.com/kddnewton/prettier-plugin-brainfuck#readme",
  "dependencies": {
    "prettier": "^2.5.1"
  },
  "devDependencies": {
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.4.0",
    "jest": "^27.4.3"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    }
  },
  "jest": {
    "transform": {}
  },
  "prettier": {
    "trailingComma": "none"
  }
}
